{
  "manifest_version": 2,

  "name": "Ayushman Launcher",
  "description": "Quickly launch Ayushman Singh Chauhan's Media",
  "version": "1.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
